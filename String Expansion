import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		char str[] = in.next().toCharArray();
		int n = str.length;
		for(int i=0;i<n;i++)
		{
		   
		    if(str[i]>='1' && str[i]<='9')
		    {
		        int num = str[i]-'0';
		        for(int j=0;j<num;j++)
		        {
		            System.out.printf("%c",str[i-1]);
		        }
		    }
		}
	}
}
